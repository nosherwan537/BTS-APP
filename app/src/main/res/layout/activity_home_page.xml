<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:fillViewport="true"
        android:id="@+id/rt7t24cm26th"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="#FFFFFFFF"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/ru2djb3ip6ch"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FF9100"
                android:paddingVertical="42dp"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/ru10thhcn3s"
                    android:layout_width="match_parent"
                    android:layout_height="191dp"
                    android:layout_marginBottom="60dp"
                    android:layout_marginHorizontal="91dp"
                    android:src="@drawable/logo"
                    android:scaleType="fitXY"
                    />
                <TextView
                    android:id="@+id/r6wp7dcc6rf9"
                    android:textColor="#BE5E5E"
                    android:textSize="24sp"
                    android:layout_marginHorizontal="13dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="BAIG TRANSPORT SERVICE"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/r0mh15qd0xvz"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#BBDEFB"
                android:paddingVertical="39dp"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/r294aw2g8u4a"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_marginBottom="23dp"
                    android:layout_marginHorizontal="16dp"
                    android:orientation="horizontal">
                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/rctrzm1sjci"
                        app:shapeAppearanceOverlay="@style/cr12"
                        android:layout_width="133dp"
                        android:layout_height="121dp"
                        android:src="@drawable/fee"
                        android:scaleType="fitXY"
                        />
                    <View
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        />

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/r1v46mxt5pv5"
                        android:layout_width="142dp"
                        android:layout_height="120dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/openingdoor"
                        app:shapeAppearanceOverlay="@style/cr12" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/rbl1ikpw8uob"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_marginBottom="45dp"
                    android:layout_marginHorizontal="34dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/rmlgyttnu3i"
                        android:textColor="#FFFFFF"
                        android:textSize="14sp"
                        android:layout_marginEnd="4dp"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="FEE PAYMENT\n"
                        />

                    <TextView
                        android:id="@+id/ry00q21se7p"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="PICK-UP"
                        android:textColor="#FFFFFF"
                        android:textSize="14sp" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/r00b1sddxq003i"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_marginBottom="15dp"
                    android:layout_marginHorizontal="16dp"
                    android:orientation="horizontal">
                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/rsw6vzoyxygg"
                        app:shapeAppearanceOverlay="@style/cr12"
                        android:layout_width="133dp"
                        android:layout_height="126dp"
                        android:src="@drawable/drop"
                        android:scaleType="fitXY"
                        />
                    <LinearLayout
                        android:id="@+id/rodg1azyzqoj"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical">
                    </LinearLayout>
                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/reokz1je6lb5"
                        app:shapeAppearanceOverlay="@style/cr12"
                        android:layout_width="132dp"
                        android:layout_height="126dp"
                        android:src="@drawable/contact"
                        android:scaleType="fitXY"
                        />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ru9cvep73au"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_marginHorizontal="57dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/rm0pvp80esa"
                        android:textColor="#FFFFFF"
                        android:textSize="14sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="DROP"
                        />
                    <View
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        />
                    <TextView
                        android:id="@+id/rmclew0rm3k"
                        android:textColor="#FFFFFF"
                        android:textSize="14sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="CONTACT"
                        />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>